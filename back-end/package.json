{
   "name": "delivery-app-back-end",
   "version": "0.1.0",
   "description": "Back-end do projeto App de Delivery",
   "main": "./src/api/server.js",
   "engines": {
      "node": ">=16.0.0"
   },
   "engineStrict": true,
   "private": true,
   "scripts": {
      "start": "ts-node src/api/server.ts",
      "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/api/server.ts",
      "test": "mocha src/tests/**/*$NAME*.test.js --exit",
      "test:coverage": "nyc --all npm run test",
      "test:coverage:json": "nyc --all --reporter=json-summary npm run test",
      "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json .",
      "typeorm": "ts-node-dev node_modules/.bin/typeorm"
   },
   "dependencies": {
      "@hapi/boom": "9.1.4",
      "body-parser": "^1.19.1",
      "cors": "2.8.5",
      "dotenv": "16.0.0",
      "express": "^4.17.2",
      "express-rescue": "1.1.31",
      "jsonwebtoken": "8.5.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "^0.2.36"
   },
   "devDependencies": {
      "@types/express": "^4.17.13",
      "@types/node": "^17.0.40",
      "@typescript-eslint/eslint-plugin": "^5.27.0",
      "@typescript-eslint/parser": "^5.27.0",
      "chai": "4.3.6",
      "chai-http": "4.3.0",
      "eslint": "^8.17.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-prettier": "^4.0.0",
      "mocha": "9.2.2",
      "nyc": "15.1.0",
      "prettier": "^2.6.2",
      "sinon": "13.0.1",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   }
}
